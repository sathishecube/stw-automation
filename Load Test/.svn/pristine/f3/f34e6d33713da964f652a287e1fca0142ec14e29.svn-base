
public class egStatic {
 static int x = 60;
 static void fun() {
  System.out.println("Within Static");
 }
 public static void main(String[] args) {
  egStatic.fun();
  System.out.println(egStatic.x);
  egStatic S1 = new egStatic();
  egStatic S2 = new egStatic();
  System.out.println(S1.x);
  S1.fun();
 }
}